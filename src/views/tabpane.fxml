<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="controllers.TabPaneController">
  <tabs>
    <Tab fx:id="tbNew" text="New">
      <content>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Label fx:id="lblNewText" />
                  <ImageView fx:id="imgNew" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
      </content>
    </Tab>
    <Tab fx:id="tbOpen" text="Open">
      <content>
            <VBox alignment="CENTER">
               <children>
                  <Label fx:id="lblOpen" />
                  <ImageView fx:id="imgOpen" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
      </content>
    </Tab>
      <Tab fx:id="tbPrint" text="Print">
        <content>
            <VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT">
               <children>
                  <Label fx:id="lblPrint" />
                  <ImageView fx:id="imgPrint" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
        </content>
      </Tab>
      <Tab text="Exit">
        <content>
            <VBox>
               <children>
                  <Button fx:id="btnExit" mnemonicParsing="false" text="Exit" />
               </children>
            </VBox>
        </content>
      </Tab>
  </tabs>
</TabPane>
